begin
  int none() is return 0 end
  int one(int x) is return x end
  int two(int x, int y) is return x + y end
  int three(int x, int y, int z) is return x + y * z end
  int a = call none();
  int b = call one(0 + 1);
  int c = call two(2 + 3 + 4, 5 + 6 + 7);
  int d = call three(8 * 9 + 10 / 11, 12 * 13 + 14 / 15, 0);
  exit 0
end